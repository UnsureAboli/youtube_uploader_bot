{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noEmit": true,
    "types": [
      "@cloudflare/workers-types"
    ],
    "lib": ["ES2022", "WebWorker"],
    "allowJs": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "useDefineForClassFields": true,
    "exactOptionalPropertyTypes": false
  },
  "include": ["src/**/*"]
}
