  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/telegram-n8n-video-bot.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/telegram-n8n-video-bot/issues"
  },
  "homepage": "https://github.com/your-username/telegram-n8n-video-bot#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
